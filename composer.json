{
  "name": "acacha/l5-repository",
  "description": "Laravel 5 - Repositories to the database layer. Based on prettus/l5-repository",
  "keywords": ["laravel", "repository", "eloquent", "model", "cache","acacha"],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergi Tur Badenas",
      "email": "sergiturbadenas@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Anderson Andrade",
      "email": "contato@andersonandra.de",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "sergiturbadenas@gmail.com",
    "issues":"https://github.com/acacha/l5-repository/issues",
    "wiki":"https://github.com/acacha/l5-repository",
    "source":"https://github.com/acacha/l5-repository"
  },
  "require": {
    "illuminate/http": "~5.0",
    "illuminate/config": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/database": "~5.0",
    "illuminate/pagination": "~5.0",
    "illuminate/console": "~5.0",
    "illuminate/filesystem": "~5.0",
    "prettus/laravel-validation": "1.1.*"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\Repository\\": "src/Prettus/Repository/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "suggest": {
    "league/fractal": "Required to use the Fractal Presenter (0.12.*).",
    "robclancy/presenter": "Required to use the Presenter Model (1.3.*)"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.2-dev"
    }
  },
  "minimum-stability": "stable"
}
